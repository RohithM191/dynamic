{
    "compilerOptions": {
      "jsx": "preserve",
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "strict": true,
      "declaration": true,
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true,
      "emitDecoratorMetadata": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "next-env.d.js",
      "**/*.js",
      "**/*.jsx"
    ]
  }